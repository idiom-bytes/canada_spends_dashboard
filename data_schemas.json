        "commodity_type": {
          "type": "string",
          "description": "Type of commodity/service category",
          "example": "Services",
          "useFor": ["seriesBy", "filter"]
        },
        "commodity_code": {
          "type": "string",
          "description": "Specific commodity code",
          "example": "T005",
          "useFor": ["filter"]
        },
        "country_of_vendor": {
          "type": "string",
          "description": "2-letter country code (CA=Canada, US=United States)",
          "example": "CA",
          "useFor": ["seriesBy", "filter"]
        },
        "solicitation_procedure": {
          "type": "string",
          "description": "Method used to solicit bids (TN=Tendering, S=Standing Offer)",
          "example": "TN",
          "useFor": ["seriesBy", "filter"]
        },
        "limited_tendering_reason": {
          "type": "string",
          "description": "If limited tendering used, the reason why",
          "useFor": ["filter"]
