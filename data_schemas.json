{
  "meta": {
    "version": 1,
    "description": "Complete schema documentation for all Canada Spends Dashboard data sources",
    "lastUpdated": "2025-01-07"
  },
  "dataSources": {
    "aggregated-contracts-under-10k": {
      "displayName": "Aggregated Contracts Under $10K",
      "description": "Small government contracts under $10K aggregated by department and year. Pre-aggregated statistics rather than individual transactions.",
      "rowCount": "~500",
      "timeRange": "2016-present",
      "source": "Treasury Board of Canada Secretariat",
      "columns": {
        "year": {
          "type": "string",
          "description": "Fiscal year of the contracts (e.g., '2016', '2017')",
          "example": "2023",
          "useFor": ["groupBy"]
        },
        "contract_goods_number_of": {
          "type": "integer",
          "description": "Count of goods contracts under $10K in this year/department",
          "example": 150,
          "useFor": ["valueField"]
        },
        "contracts_goods_original_value": {
          "type": "float",
          "description": "Total original value of all goods contracts under $10K",
          "example": 450000.00,
          "useFor": ["valueField"]
        },
        "contracts_goods_amendment_value": {
          "type": "float",
          "description": "Total amendment value for goods contracts (modifications)",
          "example": 15000.00,
          "useFor": ["valueField"]
        },
        "contract_service_number_of": {
          "type": "integer",
          "description": "Count of service contracts under $10K",
          "example": 200,
          "useFor": ["valueField"]
        },
        "contracts_service_original_value": {
          "type": "float",
          "description": "Total original value of service contracts under $10K",
          "example": 600000.00,
          "useFor": ["valueField"]
        },
        "contracts_service_amendment_value": {
          "type": "float",
          "description": "Total amendment value for service contracts",
          "example": 20000.00,
          "useFor": ["valueField"]
        },
        "contract_construction_number_of": {
          "type": "integer",
          "description": "Count of construction contracts under $10K",
          "example": 25,
          "useFor": ["valueField"]
        },
        "contracts_construction_original_value": {
          "type": "float",
          "description": "Total original value of construction contracts under $10K",
          "example": 180000.00,
          "useFor": ["valueField"]
        },
        "contracts_construction_amendment_value": {
          "type": "float",
          "description": "Total amendment value for construction contracts",
          "example": 5000.00,
          "useFor": ["valueField"]
        },
        "acquisition_card_transactions_number_of": {
          "type": "integer",
          "description": "Count of government acquisition card (corporate card) transactions",
          "example": 500,
          "useFor": ["valueField"]
        },
        "acquisition_card_transactions_total_value": {
          "type": "float",
          "description": "Total value of acquisition card transactions",
          "example": 250000.00,
          "useFor": ["valueField"]
        },
        "owner_org": {
          "type": "string",
          "description": "Department/organization code (e.g., 'cbsa-asfc')",
          "example": "cbsa-asfc",
          "useFor": ["seriesBy", "filter"]
        },
        "owner_org_title": {
          "type": "string",
          "description": "Department/organization full name (bilingual, pipe-separated)",
          "example": "Canada Border Services Agency | Agence des services frontaliers du Canada",
          "useFor": ["seriesBy"]
        }
      },
      "queryExamples": [
        "Total goods contracts by year",
        "Service contract spending by department",
        "Acquisition card usage trends"
      ]
    },
    "contracts-over-10k": {
      "displayName": "Contracts Over $10K",
      "description": "Individual government contracts valued over $10K with detailed procurement information, vendor details, and contract terms.",
      "rowCount": "~230,000",
      "timeRange": "2016-present",
      "source": "Treasury Board of Canada Secretariat",
      "columns": {
        "rowid": {
          "type": "integer",
