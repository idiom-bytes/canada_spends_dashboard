        "contract_period_start": {
          "type": "date",
          "description": "Contract start date (YYYY-MM-DD)",
          "example": "2023-04-01",
          "useFor": ["groupBy"]
        },
        "delivery_date": {
          "type": "date",
          "description": "Expected delivery or completion date",
          "example": "2024-03-31",
          "useFor": []
        },
        "contract_value": {
          "type": "float",
          "description": "Total contract value including amendments. PRIMARY VALUE FIELD.",
          "example": 250000.00,
          "useFor": ["valueField"]
        },
        "original_value": {
          "type": "float",
          "description": "Original contract value before any amendments",
          "example": 200000.00,
          "useFor": ["valueField"]
        },
        "amendment_value": {
          "type": "float",
          "description": "Total value of all contract amendments/modifications",
          "example": 50000.00,
          "useFor": ["valueField"]
        },
        "comments_en": {
          "type": "string",
          "description": "English comments about the contract",
          "useFor": []
        },
        "comments_fr": {
          "type": "string",
          "description": "French comments",
          "useFor": []
        },
        "additional_comments_en": {
          "type": "string",
          "description": "English additional notes",
          "useFor": []
        },
        "additional_comments_fr": {
          "type": "string",
          "description": "French additional notes",
          "useFor": []
        },
        "agreement_type_code": {
          "type": "string",
