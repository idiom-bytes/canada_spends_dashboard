        "trade_agreement_exceptions": {
          "type": "string",
          "description": "Whether trade agreement exceptions apply",
          "useFor": ["filter"]
        },
        "indigenous_business": {
          "type": "string",
          "description": "Whether vendor is an indigenous-owned business",
          "example": "N",
          "useFor": ["seriesBy", "filter"]
        },
        "indigenous_business_excluding_psib": {
          "type": "string",
          "description": "Indigenous business status excluding PSIB program",
          "useFor": ["filter"]
        },
        "intellectual_property": {
          "type": "string",
          "description": "Whether intellectual property is involved",
          "useFor": ["filter"]
        },
        "potential_commercial_exploitation": {
          "type": "string",
          "description": "Whether commercial use is possible",
          "useFor": ["filter"]
        },
        "former_public_servant": {
          "type": "string",
          "description": "Whether vendor employs former public servants",
          "useFor": ["filter"]
        },
        "contracting_entity": {
          "type": "string",
          "description": "Government entity conducting the procurement",
          "useFor": ["seriesBy", "filter"]
        },
        "standing_offer_number": {
          "type": "string",
          "description": "Reference number if contract is against a standing offer",
          "useFor": ["filter"]
        },
        "instrument_type": {
          "type": "string",
          "description": "Type of procurement instrument used",
          "useFor": ["seriesBy", "filter"]
        },
        "ministers_office": {
          "type": "string",
          "description": "Whether contract is for a Minister's office",
          "useFor": ["filter"]
        },
        "number_of_bids": {
          "type": "integer",
          "description": "Number of bids received in the procurement",
          "example": 5,
          "useFor": ["valueField"]
        },
        "article_6_exceptions": {
          "type": "string",
          "description": "Whether Article 6 exceptions applied",
          "useFor": ["filter"]
        },
        "award_criteria": {
          "type": "string",
